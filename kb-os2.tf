/def -i key_alt_up = /dokey RECALLBEG
/def -i key_alt_down = /dokey RECALLEND
/def -i key_alt_right = /kb_kill_word
/def -i key_alt_left = /kb_backward_kill_word
/def -i key_alt_N = /dokey SOCKETF
/def -i key_alt_P = /dokey SOCKETB
/def -i key_alt_home = /kb_backward_kill_line
