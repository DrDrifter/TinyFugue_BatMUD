/def -i key_alt_pgdn = /dokey FLUSH
/def -i key_alt_K = /recordline -i $(/recall -i 1)%;/dokey DLINE
/def -i key_alt_L = /dokey LINE
/def -i key_alt_D = /dokey DWORD	
